pipeline {

   environment {
     GREEN="\033[1;32m"
     RED="\033[1;31m"
     BLUE="\033[1;34m"
     Respect="Hello-Sir"
   }

   agent any 

   stages {
    
    stage('username') {
      steps {
          echo "${Respect} Please Enter your username"
    }
   }
    stage('password') {
       steps {
          echo -e "${GREEN} Please Enter your username" 
         }
      }
    stage('Email') {
       steps {
          echo "${Respect} Please Enter your username" 
       }
     }
   }
}